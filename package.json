{
  "name": "ste-scpb-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:report": "playwright show-report",
    "prepare": "husky",
    "lighthouse": "echo 'Run: npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags=\"--headless\"'",
    "lighthouse:ci": "lhci autorun"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@react-three/postprocessing": "^3.0.4",
    "@sanity/client": "^7.13.2",
    "@sanity/image-url": "^2.0.2",
    "@sentry/nextjs": "^10.32.1",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.8",
    "gsap": "^3.14.2",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.17.0",
    "next": "16.1.0",
    "next-intl": "^4.6.1",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "react-map-gl": "^8.1.0",
    "resend": "^6.6.0",
    "three": "^0.182.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.182.0",
    "@vitest/coverage-v8": "^4.0.16",
    "axe-core": "^4.11.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "eslint-config-prettier": "^10.1.8",
    "fast-check": "^4.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.16",
    "zod": "^4.2.1"
  }
}
